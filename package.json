{
  "name": "webpizza-poc",
  "version": "0.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "postbuild": "node scripts/generate-version.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "node patch-transformers.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "browser": {
    "onnxruntime-node": false,
    "sharp": false,
    "detect-libc": false,
    "fs": false,
    "path": false,
    "child_process": false,
    "os": false,
    "stream": false,
    "util": false,
    "events": false
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@mlc-ai/web-llm": "^0.2.79",
    "@xenova/transformers": "^2.17.2",
    "onnxruntime-web": "^1.23.2",
    "pdfjs-dist": "^5.4.296",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "typescript": "~5.9.2",
    "util": "^0.12.5"
  }
}
